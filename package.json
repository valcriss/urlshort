{
  "name": "urlshort",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "test": "jest --passWithNoTests",
    "prepare": "husky",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:clear": "prisma migrate reset --force --skip-seed"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "jose": "^5.9.2",
    "pino": "^9.4.0",
    "pino-http": "^10.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "prisma": "^5.18.0",
    "pino-pretty": "^11.2.2",
    "supertest": "^7.0.0",
    "tsx": "^4.16.2",
    "ts-jest": "^29.2.4",
    "typescript": "^5.5.4"
  }
}
